{
  "name": "idp-profile-ui",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/silinternational/idp-profile-ui"
  },
  "scripts": {
    "serve": "vite",
    "serve:prod": "vite preview",
    "build": "echo UI_VERSION=${UI_VERSION} >> .env; vite build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue}\"",
    "fix": "eslint --fix \"src/**/*.{js,vue}\"",
    "lint": "eslint \"src/**/*.{js,vue}\""
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@simplewebauthn/browser": "^4.1.0",
    "axios": "^1.8.3",
    "date-fns": "^2.28.0",
    "sass-loader": "^10.1.1",
    "vite": "^6.2.6",
    "vite-plugin-vuetify": "^2.1.0",
    "vue": "^3",
    "vue-i18n": "^10",
    "vue-router": "^4",
    "vuetify": "^3.7.16",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@sentry/vite-plugin": "^2.22.7",
    "@sentry/vue": "^7.113.0",
    "@simplewebauthn/browser": "^9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "eslint": "^9.22.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-vuetify": "^2.5.1",
    "prettier": "^3.5.3",
    "rimraf": "^3.0.2",
    "sass": "^1.85.1",
    "unplugin-vue-components": "^28.4.1",
    "vue-sanitize-directive": "^0.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "author": "SIL GTIS <gtis_itse_support+idp@sil.org>",
  "license": "MIT",
  "description": "Identity management UI"
